{
  "name": "md2ob",
  "version": "0.0.1",
  "description": "Converts a list of markdown files to OpenBook JSON schema",
  "repository": "git@github.com:botpress/md2ob.git",
  "author": "Sylvain Perron <sylvain.perron@botpress.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "test": "ava",
    "build": "del-cli dist && tsc",
    "prepack": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "openbook",
    "botpress"
  ],
  "devDependencies": {
    "@types/node": "^18.0.3",
    "ava": "^4.3.0",
    "del-cli": "^4.0.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
